{
  "name": "UDP Flood Attack",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash",
    "terminal.integrated.shellArgs.linux": ["-c"]
  },
  "extensions": [
    "ms-python.python"  // Keep if needed for additional scripts or debugging
  ],
  "forwardPorts": [
    {
      "protocol": "udp",
      "port": 80
    }
  ]
}
